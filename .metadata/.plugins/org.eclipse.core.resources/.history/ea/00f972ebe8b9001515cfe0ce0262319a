package com.fed.test;

public class Test {

	public static void main(String[] args) {
		String tmp = "asfkasdklasdkasa";
		System.out.println(tmp.length());
		
		int modulo = tmp.length()/3;
		if(modulo == 0) {
			int size = tmp.length()/3;
			for(int i=0; i<3; i++) {
				int start = (i*size);
				System.out.println(tmp.substring(start, (start+(size))));
			}
		}
		
		
	}

}
